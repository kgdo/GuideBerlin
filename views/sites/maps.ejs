<% layout("layouts/boilerplate")%>

<div class="cover-container d-flex w-100 h-100 p-3 mx-auto flex-column">
    <div id="carouselExample" class="carousel slide" >
      <div class="carousel-inner">
        <div class="carousel-item active">
          <img src="https://res.cloudinary.com/kendo/image/upload/v1693457518/27693ADC-2BCB-41D3-8FE9-BAE1BFDFEFE7_bn37jk.jpg" class="d-block w-100" alt="AlexPlatzBerlin">
        </div>
        <div class="carousel-item">
          <img src="https://res.cloudinary.com/kendo/image/upload/v1693457807/Stadtschloss_1945_battle_berlin_Soviet_Soldiers_gobvat.jpg" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
          <img src="https://res.cloudinary.com/kendo/image/upload/v1687772641/guideberlin/ptyn5shdaupkzbi33mts.png" class="d-block w-100" alt="...">
        </div>
        <div class="carousel-item">
          <img src="https://res.cloudinary.com/kendo/image/upload/v1687772353/guideberlin/n5k7dksgxztlnpxij1mt.jpg" class="d-block w-100" alt="...">
        </div>
      </div>
      <button class="carousel-control-prev" type="button" data-bs-target="#carouselExample" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button class="carousel-control-next" type="button" data-bs-target="#carouselExample" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>